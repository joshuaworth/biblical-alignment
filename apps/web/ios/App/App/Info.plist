	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>org.biblicalalignment.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>biblicalalignment</string>
			</array>
		</dict>
	</array>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>biblicalalignment</string>
	</array>
